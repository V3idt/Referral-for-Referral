# Auto detect text files and perform LF normalization
* text=auto

# Force LF for all text files
*.* text eol=lf

# Binary files
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.svg binary
*.woff binary
*.woff2 binary

# Exclude files from export
.gitattributes export-ignore
.gitignore export-ignore
.env.example export-ignore

